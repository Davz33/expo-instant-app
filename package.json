{
  "name": "expo-instant-app",
  "version": "1.0.2",
  "description": "A plugin made for enabling instant-app modifications of `AndroidManifes.xml` and other necessary ones, in order to make your expo-project instant-app compatible.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "android",
    "plugin/build",
    "build",
    "expo-module.config.json",
    "app.plugin.js"
  ],
  "scripts": {
    "ncu": "bun x npm-check-updates -p bun",
    "build": "bun x tsc --build ./ ./plugin",
    "dev": "bun x tsc --build ./ ./plugin --watch",
    "clean": "expo-module clean",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "author": "Davide Vitiello, Bitetechnology",
  "license": "ISC",
  "dependencies": {
    "@expo/config": "8.1.2",
    "@expo/config-plugins": "7.2.5",
    "@expo/plist": "^0.0.20",
    "eslint": "8.51.0",
    "expo": "49.0.13",
    "expo-config": "^1.0.0",
    "expo-module-scripts": "3.0.12",
    "jest": "29.7.0",
    "jest-expo": "49"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/react": "~18.2.23",
    "@types/react-native": "0.72.3",
    "expo-modules-core": "^1.5.11",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "expo": "49.0.13",
    "react": "*",
    "react-native": "*"
  },
  "repository": {
    "type": "git",
    "url": "github.com/Davz33/expo-instant-app"
  },
  "keywords": [
    "expo",
    "instant",
    "app",
    "android",
    "appclip",
    "react-native"
  ]
}
